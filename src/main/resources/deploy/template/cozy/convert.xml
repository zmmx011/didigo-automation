<?xml version="1.0" encoding="UTF-8"?>
<workbook>
	<worksheet idx="0">
		<section startRow="0" endRow="3"/>
		<loop startRow="4" endRow="4" items="items" var="item" varType="com.invenia.excel.converter.dto.Item">
			<section startRow="4" endRow="4">
				<mapping row="4" col="0">item.idxNo</mapping>
				<mapping row="4" col="2">item.orderDate</mapping>
				<mapping row="4" col="4">item.itemNo</mapping>
				<mapping row="4" col="4">item.itemName</mapping>
				<mapping row="4" col="6">item.qty</mapping>
				<mapping row="4" col="7">item.price</mapping>
			</section>
			<loopbreakcondition>
				<rowcheck offset="0">
					<cellcheck offset="5">총잔수</cellcheck>
				</rowcheck>
			</loopbreakcondition>
		</loop>
	</worksheet>
</workbook>
