<?xml version="1.0" encoding="UTF-8"?>
<workbook>
  <worksheet idx="0" name="Sheet1">
    <loop endRow="3" items="items" startRow="3" var="item"
      varType="com.invenia.excel.converter.dto.Item">
      <loopbreakcondition>
        <rowcheck offset="0"/>
      </loopbreakcondition>
      <section endRow="3" startRow="3">
        <mapping col="2" row="3">item.orderDate</mapping>
        <mapping col="4" row="3">item.itemClassSName</mapping>
        <mapping col="11" row="3">item.itemName</mapping>
        <mapping col="12" row="3">item.spec</mapping>
        <mapping col="13" row="3">item.itemNo</mapping>
        <mapping col="14" row="3">item.smInOutKindName</mapping>
        <mapping col="19" row="3">item.unitName</mapping>
        <mapping col="20" row="3">item.assetName</mapping>
        <mapping col="21" row="3">item.qty</mapping>
        <mapping col="22" row="3">item.price</mapping>
        <mapping col="24" row="3">item.isInclusedVat</mapping>
        <mapping col="32" row="3">item.whName</mapping>
        <mapping col="33" row="3">item.remarkM</mapping>
      </section>
    </loop>
    <section endRow="2" startRow="0"/>
  </worksheet>
</workbook>
